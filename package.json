{
  "name": "documentation-ably-realtime",
  "version": "1.0.0",
  "private": true,
  "description": "Documentation | Ably Realtime",
  "keywords": [
    "Ably"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.+(js|ts|jsx|tsx|json|css)": [
      "jest --findRelatedTests"
    ]
  },
  "engines": {
    "node": ">=16.14.2"
  },
  "scripts": {
    "develop": "export EDITOR_WARNINGS_OFF='true' && gatsby clean && gatsby develop",
    "start": "npm run clean && npm run develop",
    "edit": "unset EDITOR_WARNINGS_OFF && gatsby clean && gatsby develop",
    "build": "npm run clean && gatsby build --prefix-paths",
    "serve": "gatsby serve --prefix-paths",
    "clean": "gatsby clean",
    "rebuild": "gatsby clean && gatsby build --prefix-paths && gatsby serve --prefix-paths",
    "test": "jest -u",
    "test:ETL": "jest --testPathPattern=\"$PWD/data/*\"",
    "test:data": "npm run test:ETL",
    "test:app": "jest --testPathPattern=\"$PWD/src/*\"",
    "lint": "eslint \"src/**/*.js\" \"src/**/*.ts\" \"src/**/*.tsx\" ./gatsby-config.ts ./gatsby-node.ts",
    "lint:fix": "eslint --fix \"src/**/*.js\" \"src/**/*.ts\" \"src/**/*.tsx\" ./gatsby-config.ts ./gatsby-node.ts"
  },
  "dependencies": {
    "@ably/ui": "7.8.2",
    "@types/cheerio": "^0.22.31",
    "@types/prop-types": "^15.7.4",
    "addsearch-js-client": "^0.7.0",
    "babel-plugin-styled-components": "^2.0.2",
    "cheerio": "^1.0.0-rc.10",
    "dompurify": "^2.3.4",
    "front-matter": "^4.0.2",
    "fs-extra": "^10.0.1",
    "gatsby": "^4.16.0",
    "gatsby-plugin-client-side-redirect": "^1.1.0",
    "gatsby-plugin-image": "^2.3.0",
    "gatsby-plugin-manifest": "^4.9.1",
    "gatsby-plugin-react-helmet": "^5.16.0",
    "gatsby-plugin-root-import": "^2.0.8",
    "gatsby-plugin-sharp": "^4.3.0",
    "gatsby-plugin-sitemap": "^5.3.0",
    "gatsby-plugin-styled-components": "^5.3.0",
    "gatsby-plugin-ts": "^3.1.1",
    "gatsby-source-filesystem": "^4.3.0",
    "gatsby-transformer-sharp": "^4.3.0",
    "gatsby-transformer-yaml": "^4.4.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.1.7",
    "react": "^17.0.1",
    "react-accessible-accordion": "^4.0.0",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-select": "^5.2.2",
    "react-syntax-highlighter": "^15.4.5",
    "styled-components": "^5.3.3",
    "tailwind": "^2.4.1",
    "tailwindcss-filters": "^2.0.0",
    "textile-js": "^2.1.1",
    "typescript": "^4.6.3",
    "util": "^0.12.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/dompurify": "^2.3.3",
    "@types/jest": "^28.1.2",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash.debounce": "^4.0.7",
    "@types/react-helmet": "^6.1.5",
    "@types/react-select": "^5.0.1",
    "@types/react-syntax-highlighter": "^15.5.4",
    "@types/styled-components": "^5.1.25",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.4.6",
    "babel-preset-gatsby": "^2.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "fast-check": "^2.21.0",
    "gatsby-plugin-postcss": "^5.4.0",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^17.0.2",
    "source-map": "^0.7.4",
    "tailwindcss": "^2.2.19"
  }
}
